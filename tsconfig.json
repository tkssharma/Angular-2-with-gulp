{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false
  },
  "fileGlobs": [
    "./manual_typings/custom.d.ts",
    "./typings/browser.d.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": false
}
